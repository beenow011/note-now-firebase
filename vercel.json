{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      { "src": "/", "dest": "/" },
      { "src": "/login", "dest": "/" },
      { "src": "/signup", "dest": "/" },
      { "src": "/notes", "dest": "/" },
      { "src": "/notebook/:notesId", "dest": "/" }
    ]
  }
  